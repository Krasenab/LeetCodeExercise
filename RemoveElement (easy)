public class Solution {
    public int RemoveElement(int[] nums, int val) 
    {
        int k = 0;
        for(int a =0;a<nums.Length;a++)
        {
            int currentVal = nums[a];
            if(val!=currentVal)
            {
                nums[k] = currentVal;
                k++;
            }
                
        }       
        return k;
        
    }
}
